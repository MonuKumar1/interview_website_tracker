<h1 style="text-align:center;">Most Important Topics ðŸ˜‹ </h1>
<h2 style="text-align:center;">
    <br> 
<% topic.forEach(function(topics){ %>
<li><a href="/question" onclick=getTopic(this)><%= topics.topic %></a></li>
<% }) %>
</h2>
<script>
    async function  getTopic(id){
         const topic =  id.innerText;
         //modify url

         //get topic
         try {
             const res = await fetch('/topic',{
                 method: 'POST',
                 body: JSON.stringify({topic}),
                 headers:{'Content-Type': 'application/json'}
             })


         } catch (error) {
             console.log(error);
         }
     }
     
 </script>

